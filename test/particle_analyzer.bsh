import ij.plugin.filter.ParticleAnalyzer;
import ij.*;
import ij.measure.ResultsTable;
import ij.plugin.frame.RoiManager;
import ij.measure.Measurements;
double minCellSize = 20/(0.11*0.11);
double maxCellSize = 200/(0.11*0.11);
ResultsTable resultTable = new ResultsTable();
RoiManager roiManager = new RoiManager();
ParticleAnalyzer particleAnalyzer = new ParticleAnalyzer(
		ParticleAnalyzer.EXCLUDE_EDGE_PARTICLES
				+ ParticleAnalyzer.SHOW_PROGRESS
				+ ParticleAnalyzer.ADD_TO_MANAGER,
		Measurements.AREA + Measurements.CENTROID
				+ Measurements.PERIMETER
				+ Measurements.SHAPE_DESCRIPTORS + Measurements.ELLIPSE,
		resultTable, minCellSize, maxCellSize);

particleAnalyzer.analyze(IJ.getImage());
print("Done");